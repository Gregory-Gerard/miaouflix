// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./miaouflix.db"
}

model Movie {
  id         Int        @id
  title      String
  m3u8       String?
  categories Category[]
}

model Category {
  id     Int     @id @default(autoincrement())
  title  String  @unique
  movies Movie[]
  order  Int     @default(0)
}
